{
	"manifest_version": 2,
	"name": "CWP",
	"version": "1.0",
	"background": {
		"scripts": [
			"./import/web3.js",
			"./import/ethereumjs-tx-1.3.3.js",
			"background.js",
			"./js/wallet.js"
		]
	},
	"browser_action": {
		"default_icon": "happy.png",
		"default_popup": "popup.html",
		"default_title": "A popup will come here"
	},
	"permissions": [
		"storage"
	],
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com 'unsafe-eval' ; object-src 'self'"
}